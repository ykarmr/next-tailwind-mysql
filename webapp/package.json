{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest --watch",
    "format": "prettier --write './components/**/*.{js,jsx,ts,tsx}' './pages/**/*.{js,jsx,ts,tsx}'",
    "prisma:init": "npx prisma migrate dev --name init",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:seed": "ts-node --compilerOptions '{\"isolatedModules\":false}' prisma/seed.ts",
    "prisma:format": "prisma format --schema prisma/schema.prisma",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "4.3.1",
    "autoprefixer": "^10.4.8",
    "axios": "^0.27.2",
    "fp-ts": "^2.12.3",
    "io-ts": "^2.2.18",
    "next": "12.2.5",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.16",
    "prisma": "^4.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.35.0",
    "tailwindcss": "^3.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "18.7.15",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "babel-loader": "^8.2.5",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "prettier": "^2.7.1",
    "typescript": "4.8.2"
  },
  "exclude": [
    "prisma/seed.ts"
  ]
